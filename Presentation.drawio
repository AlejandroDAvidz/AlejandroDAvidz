        <mxCell id="f6ZDDn2nSGfMl3vf_n1B-18" value="&lt;meta charset=&quot;utf-8&quot;&gt;Croissance des entreprises blockchain" style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="1">
